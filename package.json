    "@rollup/plugin-node-resolve": "8.0.1",
    "@types/jest": "26.0.0",
    "@types/styled-components": "^5.1.0",
    "core-js": "3.6.5",
    "jest": "26.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
