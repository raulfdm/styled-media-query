  "main": "dist/index.cjs.js",
  "browser": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "repository": "git@github.com:morajabi/styled-media-query.git",
  "keywords": [
    "breakpoint",
    "emotion",
    "media-query",
    "media",
    "styled-components",
    "utils"
  ],
